{% extends 'base.html.twig' %}

{% block title %}Education index
{% endblock %}

{% block body %}
	<h1>Education index</h1>
	{% block stylesheets %}
		{{ encore_entry_link_tags('crud') }}
	{% endblock %}
	<table class="table">
		<thead>
			<tr>
				<th>Id</th>
				<th>Name</th>
				<th>Category</th>
				<th>Debut</th>
				<th>Fin</th>
				<th>Description</th>
				<th>actions</th>
			</tr>
		</thead>
		<tbody>
			{% for education in education %}
				<tr>
					<td>{{ education.id }}</td>
					<td>{{ education.name }}</td>
					<td>{{ education.category }}</td>
					<td>{{ education.debut }}</td>
					<td>{{ education.fin }}</td>
					<td>{{ education.description }}</td>
					<td>
						<a href="{{ path('app_education_show', {'id': education.id}) }}">show</a>
						<a href="{{ path('app_education_edit', {'id': education.id}) }}">edit</a>
					</td>
				</tr>
			{% else %}
				<tr>
					<td colspan="7">no records found</td>
				</tr>
			{% endfor %}
		</tbody>
	</table>

	<a href="{{ path('app_education_new') }}">Create new</a>
{% endblock %}
