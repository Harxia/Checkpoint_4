{% extends 'base.html.twig' %}

{% block title %}Education
{% endblock %}

{% block body %}
	{% block stylesheets %}
		{{ encore_entry_link_tags('crud') }}
	{% endblock %}
	<h1>Education</h1>

	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ education.id }}</td>
			</tr>
			<tr>
				<th>Name</th>
				<td>{{ education.name }}</td>
			</tr>
			<tr>
				<th>Category</th>
				<td>{{ education.category }}</td>
			</tr>
			<tr>
				<th>Debut</th>
				<td>{{ education.debut }}</td>
			</tr>
			<tr>
				<th>Fin</th>
				<td>{{ education.fin }}</td>
			</tr>
			<tr>
				<th>Description</th>
				<td>{{ education.description }}</td>
			</tr>
		</tbody>
	</table>

	<a href="{{ path('app_education_index') }}">back to list</a>

	<a href="{{ path('app_education_edit', {'id': education.id}) }}">edit</a>

	{{ include('education/_delete_form.html.twig') }}
{% endblock %}
