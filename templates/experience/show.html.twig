{% extends 'base.html.twig' %}

{% block title %}Experience
{% endblock %}

{% block body %}
	<h1>Experience</h1>
	{% block stylesheets %}
		{{ encore_entry_link_tags('crud') }}
	{% endblock %}
	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ experience.id }}</td>
			</tr>
			<tr>
				<th>Name</th>
				<td>{{ experience.name }}</td>
			</tr>
			<tr>
				<th>Category</th>
				<td>{{ experience.category }}</td>
			</tr>
			<tr>
				<th>Debut</th>
				<td>{{ experience.debut }}</td>
			</tr>
			<tr>
				<th>Fin</th>
				<td>{{ experience.fin }}</td>
			</tr>
			<tr>
				<th>Description</th>
				<td>{{ experience.description }}</td>
			</tr>
		</tbody>
	</table>

	<a href="{{ path('app_experience_index') }}">back to list</a>

	<a href="{{ path('app_experience_edit', {'id': experience.id}) }}">edit</a>

	{{ include('experience/_delete_form.html.twig') }}
{% endblock %}
