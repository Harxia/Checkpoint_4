{% extends 'base.html.twig' %}

{% block title %}Experience index
{% endblock %}

{% block body %}
	{% block stylesheets %}
		{{ encore_entry_link_tags('crud') }}
	{% endblock %}
	<h1>Experience index</h1>

	<table class="table">
		<thead>
			<tr>
				<th>Id</th>
				<th>Name</th>
				<th>Category</th>
				<th>Debut</th>
				<th>Fin</th>
				<th>Description</th>
				<th>actions</th>
			</tr>
		</thead>
		<tbody>
			{% for experience in experiences %}
				<tr>
					<td>{{ experience.id }}</td>
					<td>{{ experience.name }}</td>
					<td>{{ experience.category }}</td>
					<td>{{ experience.debut }}</td>
					<td>{{ experience.fin }}</td>
					<td>{{ experience.description }}</td>
					<td>
						<a href="{{ path('app_experience_show', {'id': experience.id}) }}">show</a>
						<a href="{{ path('app_experience_edit', {'id': experience.id}) }}">edit</a>
					</td>
				</tr>
			{% else %}
				<tr>
					<td colspan="7">no records found</td>
				</tr>
			{% endfor %}
		</tbody>
	</table>

	<a href="{{ path('app_experience_new') }}">Create new</a>
{% endblock %}
