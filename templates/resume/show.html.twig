{% extends 'base.html.twig' %}

{% block title %}Resume
{% endblock %}

{% block body %}
	<div class="container">
		{% block stylesheets %}
			{{ encore_entry_link_tags('crud') }}
		{% endblock %}
		<h1>Resume</h1>

		<table class="table">
			<tbody>
				<tr>
					<th>Id</th>
					<td>{{ resume.id }}</td>
				</tr>
				<tr>
					<th>Name</th>
					<td>{{ resume.name }}</td>
				</tr>
			</tbody>
		</table>

		<a href="{{ path('app_resume_index') }}">back to list</a>

		<a href="{{ path('app_resume_edit', {'id': resume.id}) }}">edit</a>

		{{ include('resume/_delete_form.html.twig') }}
	{% endblock %}
